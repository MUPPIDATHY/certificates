<!-- Modal -->
<div class="modal fade" id="bonofide" tabindex="-1" role="dialog" aria-labelledby="bonofideLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="bonofideLabel">Bonofide Certificate</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form id="loginform"  class="book-form" role="form" method="post" action="/get_report">
          <input type='text' name='cand_id' hidden  value='<%= message %>'/>
          <div class="form-group">
            <label for="exampleFormControlInput1">Reference No</label>
            <input type="text" class="form-control" name="bon_ref_no" id="exampleFormControlInput1" placeholder="">
          </div>
          <div class="form-group">
            <label for="exampleFormControlInput1">Academic Year</label>
            <input type       ="text"
            id         ="cellphone-number"class="form-control" name="bon_acad_year"
            oninput    ="onlyNumbers(this.value)"
            placeholder="Ex.: 2019-2020">
          </div>
          <div class="form-group">
            <label for="exampleFormControlSelect1">Pursing</label>
            <select class="form-control" name="bon_purse" id="exampleFormControlSelect1">
              <option>Pursing</option>
              <option>Studying</option>
              <option>Other</option>
              
            </select>
          </div>
          <div class="form-group">
            <label for="exampleFormControlSelect1">Studying year</label>
            <select class="form-control"name="bon_stud_year" id="exampleFormControlSelect1">
              <option>1st Year</option>
              <option>2nd Year</option>
              <option>3rd Year</option>
              
            </select>
          </div>
          <div class="form-group">
            <label for="exampleFormControlSelect1">Purpose</label>
            <select class="form-control" name="bon_purpose"id="exampleFormControlSelect1">
              <option value="STATE Quota">STATE Quota</option>
              <option value="2nd Round AIQ Counselling">2nd Round AIQ Counselling</option>
              <option value="Others">Others</option>
              
            </select>
          </div>
          <div class="form-group">
            <label for="exampleFormControlTextarea1">Purpose If others</label>
            <textarea class="form-control" name="bon_pur_others" id="exampleFormControlTextarea1" rows="3"></textarea>
          </div>
          <div class="modal-footer">
            <!-- <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button> -->
            <button type="submit" class="btn btn-primary">Submit </button>
          </div>
        </form>
      </div>
     
    </div>
  </div>
</div>

